{
  "name": "php-integrator-linter",
  "main": "./lib/Main",
  "version": "1.2.1",
  "description": "Lints your PHP source code to indicate various problems such as missing methods.",
  "repository": "https://github.com/php-integrator/atom-linter.git",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.8.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "getProviders"
      }
    }
  },
  "consumedServices": {
    "php-integrator.service": {
      "versions": {
        "^2.0": "setService"
      }
    },
    "linter-indie": {
      "versions": {
        "1.0.0": "setLinterIndieService"
      }
    }
  },
  "dependencies": {
    "jquery": "~2.1.4"
  },
  "keywords": [
    "php",
    "linter",
    "linting",
    "integrator",
    "integration",
    "php-integrator"
  ]
}
