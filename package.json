  "version": "0.0.0",
  "description": "Lints your PHP source code to indicate various problems such as missing methods.",
  "repository": "https://github.com/Gert-dev/php-integrator-linter.git",
  "license": "GPL-3.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
